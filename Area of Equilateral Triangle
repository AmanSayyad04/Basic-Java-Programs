package tutorial1;
import java.util.*; 

public class areaOfEqui {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		  System.out.println("Enter the side: ");
		  int b =sc.nextInt();
		  
//		  System.out.println("Enter the diagonal2: ");
//		  int h = sc.nextInt();
		  double ans = b * b * 0.43301270189;
		  
		  System.out.println("Area of Equilateral Triangle is: " + ans);

	}

}
